<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.kco.fun.activity.demo1.SimpleActivity">

    <Button android:id="@+id/start_simple"
            android:text="获取图片测试"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_photo"
            android:text="获取图片测试2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_imageRecognition"
        android:text="图片识别"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_ocrRecognition"
        android:text="ocr识别"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_alburm_all"
        android:text="相册(全部)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_alburm_chemo"
        android:text="相册(车模)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_alburm_mingxing"
        android:text="相册(明星)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_alburm_qingchun"
        android:text="相册(清纯)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_alburm_qipao"
        android:text="相册(旗袍)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button android:id="@+id/start_alburm_xiaohua"
        android:text="相册(校花)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
